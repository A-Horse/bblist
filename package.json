{
  "name": "octopus",
  "version": "0.0.0",
  "private": true,
  "description": "octopus",
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./delopy/webpack.prod.js --progress --profile",
    "start": "NODE_ENV=development webpack-dev-server --config ./delopy/webpack.dev.js --history-api-fallback --watch --host 0.0.0.0",
    "test": "jest --ci",
    "analyze": "webpack --config ./delopy/webpack.prod.js --json | webpack-bundle-size-analyzer",
    "proxy-built": "node delopy/proxy.js"
  },
  "license": "BSD",
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/setup/localStorage.js",
      "<rootDir>/test/setup/media.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^babel-loader": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "endpoint$": "<rootDir>/__mocks__/endpoint.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
