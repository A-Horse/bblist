{
  "presets": ["env", "react"],
  "plugins": [
    ["import", [
      {
        "libraryName": "antd"
      }
    ]],
    "ramda",
    "transform-flow-strip-types",
    "transform-object-rest-spread",
    "syntax-class-properties",
    "transform-class-properties",
    "transform-decorators-legacy"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
